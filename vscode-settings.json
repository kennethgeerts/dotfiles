{
  "alignment.surroundSpace": { "colon": [-1, 1] },
  "breadcrumbs.enabled": false,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorSurroundingLines": 5,
  "editor.folding": false,
  "editor.fontFamily": "Operator Mono SSm",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": "200",
  "editor.glyphMargin": false,
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.renderControlCharacters": true,
  "editor.renderFinalNewline": false,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "none",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": { "textMateRules": [{ "scope": ["entity.other.attribute-name"], "settings": { "fontStyle": "italic" } }] },
  "emmet.includeLanguages": { "erb": "html" },
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "i18n-ally.enabledFrameworks": ["ruby-rails"],
  "i18n-ally.enabledParsers": ["yaml"],
  "i18n-ally.keepFulfilled": true,
  "i18n-ally.localesPaths": ["config/locales"],
  "i18n-ally.sortKeys": true,
  "search.showLineNumbers": true,
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  "terminal.explorerKind": "external",
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Gruvbox Material Dark",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.enableExperiments": true,
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "[html]": { "editor.defaultFormatter": "vscode.html-language-features" },
  "[javascript]": { "editor.defaultFormatter": "vscode.typescript-language-features" },
  "[json]": { "editor.defaultFormatter": "vscode.json-language-features" },
  "[jsonc]": { "editor.defaultFormatter": "vscode.json-language-features" },
  "[scss]": { "editor.defaultFormatter": "HookyQR.beautify" },
}
